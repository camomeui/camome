{
  "name": "@camome/global",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "run-ts": "node -r esbuild-register",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:web": "pnpm build --scope=@camome/web",
    "typecheck": "pnpm -F \"./apps/*\" -F \"./packages/*\" -r --parallel typecheck",
    "lint": "eslint packages/** apps/** --ext .ts,.tsx,.js,.jsx,.mjs,.cjs",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "jest",
    "ci:check": "concurrently \"pnpm typecheck\" \"pnpm lint\" \"pnpm format\" \"pnpm test\"",
    "ci:publish": "pnpm build --filter=\"./packages/*\" && pnpm changeset publish"
  },
  "dependencies": {
    "@mdx-js/react": "^2.1.5",
    "clsx": "^1.2.1",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "mdast-util-to-string": "^3.1.0",
    "next-mdx-remote": "^4.2.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-live": "^3.1.1",
    "react-merge-refs": "^2.0.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.0.1",
    "rehype-prism-plus": "^1.5.0",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.2",
    "remark-code-titles": "^0.1.2",
    "remark-directive": "^2.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx": "^2.1.5",
    "remark-mdx-code-meta": "^2.0.0",
    "remark-parse": "^10.0.1",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@sucrase/jest-plugin": "^3.0.0",
    "@svgr/webpack": "^6.5.1",
    "@types/jest": "^29.2.5",
    "@types/mdx": "^2.0.3",
    "@types/node": "18.11.17",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.18",
    "esbuild-register": "^3.4.1",
    "eslint": "8.29.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "globby": "^13.1.2",
    "jest": "^29.3.1",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.0",
    "prettier-plugin-md-nocjsp": "^1.5.0",
    "sucrase": "^3.29.0",
    "tsx": "^3.12.1",
    "turbo": "^1.6.3",
    "typed-scss-modules": "^7.0.1",
    "typescript": "4.9.3"
  }
}
