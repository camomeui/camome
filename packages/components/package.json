{
  "name": "@camome/components",
  "version": "0.1.14",
  "description": "Head-only React components.",
  "author": "Matsura Yuma <lemonburst1958@gmail.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "url": "https://github.com/camomeui/camome.git",
    "type": "git",
    "directory": "packages/components"
  },
  "type": "module",
  "scripts": {
    "gen-scss-type": "typed-scss-modules \"**/*.module.scss\" --aliasPrefixes.@ ./ --nameFormat none --exportType default --ignore \"node_modules\" --logLevel error",
    "dev:vite": "vite build --watch",
    "dev": "concurrently \"pnpm gen-scss-type -w \" \"pnpm dev:vite\" \"pnpm storybook\"",
    "build": "vite build",
    "typecheck": "pnpm gen-scss-type && tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "run-ts": "node -r esbuild-register"
  },
  "dependencies": {
    "@camome/system": "workspace:*",
    "@camome/utils": "workspace:*",
    "@heroicons/react": "^2.0.10",
    "clsx": "^1.2.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.0-beta.15",
    "@storybook/addon-interactions": "^7.0.0-beta.15",
    "@storybook/addon-links": "^7.0.0-beta.15",
    "@storybook/blocks": "^7.0.0-alpha.8",
    "@storybook/jest": "^0.0.10",
    "@storybook/react": "^7.0.0-beta.15",
    "@storybook/react-vite": "^7.0.0-alpha.25",
    "@storybook/testing-library": "^0.0.13",
    "@types/body-scroll-lock": "^3.1.0",
    "@vitejs/plugin-react": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.55.0",
    "storybook": "^7.0.0-beta.15",
    "vite": "^4.0.2",
    "vite-plugin-dts": "^1.7.1",
    "vite-tsconfig-paths": "^4.0.3"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    "./*": {
      "import": "./src/components/*/index.tsx"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/components/*/index.d.ts"
      ]
    }
  },
  "sideEffects": false
}
