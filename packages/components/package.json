{
  "name": "@camome/components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "gen-scss-type": "typed-scss-modules \"**/*.module.scss\" --aliasPrefixes.@ ./ --nameFormat none --exportType default --ignore \"node_modules\" --logLevel error",
    "dev:vite": "vite build --watch",
    "dev": "concurrently \"pnpm gen-scss-type -w \" \"pnpm dev:vite\"",
    "build": "vite build"
  },
  "dependencies": {
    "@camome/utils": "workspace:*",
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.10",
    "body-scroll-lock": "4.0.0-beta.0"
  },
  "devDependencies": {
    "@types/body-scroll-lock": "^3.1.0",
    "@vitejs/plugin-react": "^3.0.0",
    "sass": "^1.55.0",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1"
  },
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": false
}
