{
  "name": "@camome/web",
  "version": "0.1.29",
  "private": true,
  "type": "module",
  "description": "Light weight, accessibleUI framework for React and CSS",
  "homepage": "https://camome.net",
  "author": {
    "name": "Matsura Yuma",
    "email": "lemonburst1958@gmail.com",
    "url": "https://rubiq.vercel.app"
  },
  "scripts": {
    "run-ts": "node -r esbuild-register",
    "watch-stories": "pnpm run-ts ./scripts/watch-stories.ts",
    "dev:next": "next dev --port 3910",
    "dev": "concurrently \"pnpm gen-scss-type -w \" \"pnpm dev:next\"",
    "build": "pnpm gen-scss-type && pnpm gen-docs-data && next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "pnpm gen-scss-type && tsc",
    "gen-scss-type": "typed-scss-modules \"src/**/*.module.scss\" --aliasPrefixes.@ src --outputFolder .scss-types --nameFormat none --exportType default --logLevel error",
    "gen-docs-data": "tsx ./scripts/gen-docs-data.ts"
  },
  "dependencies": {
    "@camome/core": "workspace:*",
    "@camome/system": "workspace:*",
    "@camome/utils": "workspace:*",
    "@floating-ui/react": "^0.19.1",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@headlessui/react": "1.7.7",
    "@heroicons/react": "^2.0.14",
    "@mdx-js/loader": "^2.1.5",
    "@mdx-js/react": "^2.1.5",
    "@next/bundle-analyzer": "^13.1.6",
    "@next/mdx": "^13.1.6",
    "@svgr/webpack": "^6.5.1",
    "camome": "workspace:*",
    "contentlayer": "^0.3.0",
    "esbuild-sass-plugin": "^2.4.5",
    "form-data": "^4.0.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "image-size": "^1.0.2",
    "lodash.sortby": "^4.7.0",
    "mailgun.js": "^8.0.1",
    "markdown-wasm": "^1.2.0",
    "mdast-util-to-string": "^3.1.0",
    "next": "13.1.6",
    "next-contentlayer": "^0.3.0",
    "next-mdx-remote": "^4.2.0",
    "next-seo": "^5.5.0",
    "next-themes": "^0.2.1",
    "next-translate": "^1.6.0",
    "prism-react-renderer": "^1.3.5",
    "react-hook-form": "^7.43.0",
    "react-icons": "^4.4.0",
    "react-wrap-balancer": "^0.4.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.0.1",
    "rehype-prism-plus": "^1.5.0",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.2",
    "remark-code-titles": "^0.1.2",
    "remark-directive": "^2.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx": "^2.1.5",
    "remark-mdx-code-meta": "^2.0.0",
    "remark-parse": "^10.0.1",
    "sharp": "^0.31.3",
    "undent": "^0.1.0",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1"
  },
  "devDependencies": {
    "@camome/storybook": "workspace:*",
    "@types/lodash.sortby": "^4.7.7",
    "@types/mdx": "^2.0.3",
    "@types/node": "18.11.18",
    "@types/yargs": "^17.0.22",
    "autoprefixer": "^10.4.13",
    "chokidar": "^3.5.3",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "hastscript": "^7.1.0",
    "postcss": "^8.4.20",
    "postcss-modules": "^6.0.0",
    "postcss-preset-env": "^8.0.1",
    "react-docgen": "6.0.0-alpha.4",
    "react-docgen-typescript": "^2.2.2",
    "sass": "^1.58.0",
    "yargs": "^17.6.2"
  }
}
