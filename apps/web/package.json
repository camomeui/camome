{
  "name": "@camome/web",
  "version": "0.1.4",
  "private": true,
  "type": "module",
  "scripts": {
    "gen-scss-type": "typed-scss-modules \"src/**/*.module.scss\" --aliasPrefixes.@ src --outputFolder .scss-types --nameFormat none --exportType default --logLevel error",
    "watch-stories": "pnpm run-ts ./scripts/watch-stories.ts",
    "dev:next": "next dev --port 3910",
    "dev": "concurrently \"pnpm gen-scss-type -w \" \"pnpm dev:next\"",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "tsc",
    "generate-css": "npx camome generate-css --output ./src/styles/camome.css",
    "run-ts": "node -r esbuild-register"
  },
  "dependencies": {
    "@camome/components": "workspace:*",
    "@camome/system": "workspace:*",
    "@camome/utils": "workspace:*",
    "@floating-ui/react": "^0.14.0",
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.10",
    "@mdx-js/loader": "^2.1.5",
    "@mdx-js/react": "^2.1.5",
    "@next/mdx": "^13.0.0",
    "camome": "workspace:*",
    "contentlayer": "^0.2.9",
    "form-data": "^4.0.0",
    "image-size": "^1.0.2",
    "mailgun.js": "^8.0.1",
    "next": "13.0.6",
    "next-contentlayer": "^0.2.9",
    "next-seo": "^5.5.0",
    "next-themes": "^0.2.1",
    "react-hook-form": "^7.40.0",
    "react-icons": "^4.4.0",
    "rehype-external-links": "^2.0.1",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "18.7.14",
    "chokidar": "^3.5.3",
    "eslint": "8.23.0",
    "eslint-config-next": "13.0.0",
    "hastscript": "^7.1.0",
    "next-transpile-modules": "^10.0.0",
    "postcss": "^8.4.20",
    "postcss-preset-env": "^7.8.3"
  }
}
